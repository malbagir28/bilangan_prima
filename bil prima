<?php
$bil = 0;
  for ($i=1; $i <= 40 ; $i++) {
    $s = 0;
      for ($j=1; $j <= 40 ; $j++) {
        if ($i % $j == 0) {
          $s++;
        }
      }
      if ($s == 2) {
        $bil++;
        echo $i." ";
          if ($bil % 3 == 0) {
            echo "<br>";
          }
      }
  }